<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--namespace=绑定一个对应的Dao/Mapper接口-->
<mapper namespace="ddd.leave.domain.leave.repository.mapper.ApprovalInfoMapper">
    <!--select查询语句-->
    <select id="queryByLeaveId" resultType="ddd.leave.domain.leave.repository.po.ApprovalInfoPO">
        select * from t_approval_info where leave_id=#{leaveId};
    </select>
</mapper>